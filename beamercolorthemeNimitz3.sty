%It's easy to imagine what color and font themes do. inner themes define how elements inside a slide (items, enumerate, blocks, ...) look while outer themes define headlines, footlines, sidebar, ...

\mode<presentation>

% define colors
\definecolor{lightblue}{RGB}{198, 217, 241}
\definecolor{mediumblue}{RGB}{0, 103, 182}
\definecolor{darkblue}{HTML}{005493}
\definecolor{gold}{HTML}{f4da40}
%\definecolor{lightgray}{RGB}{234, 237, 242}
\definecolor{darkgray}{HTML}{3465a4}
%\definecolor{darkestblue}{HTML}{254061} % for outlines of boxes
\definecolor{linkblue}{HTML}{0000FF} % for url and email addresses

% Settings
\setbeamercolor{background canvas}{bg=lightblue}

\setbeamercolor{headline}{fg=white,bg=mediumblue}
\setbeamercolor{title in headline}{fg=white}
\setbeamercolor{author in headline}{fg=white}
\setbeamercolor{institute in headline}{fg=white}
\setbeamercolor*{title page header}{fg=white}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{date}{fg=white}
\setbeamercolor{lower separation line}{fg=darkblue}

